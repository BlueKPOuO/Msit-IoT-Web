@model IoTWeb.Models.Location

@if (Model.Photo != null )
{
    <img src="@Url.Action("ShowPhoto","Locations",new { id=Model.LocationID})"
         style="width:320px;height:240px" title="@Html.DisplayFor(Model=>Model.Location1)" />

}
else
{
    <img src="@Url.Content("~/Image/no-image.png")" style="width:320px;height:240px" />
}


